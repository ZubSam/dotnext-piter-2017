{
  "dependencies": {
    "Microsoft.AspNetCore.Mvc": "1.1.2",
    "Microsoft.AspNetCore.Routing": "1.1.1",
    "Microsoft.AspNetCore.Server.Kestrel": "1.1.1",
    "Microsoft.AspNetCore.Mvc.Versioning": "1.1.0-beta2",
    "Microsoft.AspNetCore.Mvc.Versioning.ApiExplorer": "1.0.0-beta1",
    "Serilog": "2.4.0",
    "Serilog.Extensions.Logging": "1.4.0",
    "Serilog.Settings.Configuration": "2.3.1",
    "Serilog.Sinks.Literate": "2.1.0",
    "Serilog.Enrichers.Thread": "3.0.0",
    "Swashbuckle.AspNetCore": "1.0.0"
  },

  "frameworks": {
    "netcoreapp1.1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.1.1"
        }
      }
    }
  },

  "buildOptions": {
    "emitEntryPoint": true
  },

  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true,
      "System.GC.Concurrent": true,
      "System.Threading.ThreadPool.MinThreads": 1,
      "System.Threading.ThreadPool.MaxThreads": 2
    }
  },

  "publishOptions": {
    "include": []
  }
}
